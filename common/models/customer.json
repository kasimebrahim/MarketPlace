{
  "name": "Customer",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "accountId": {
      "type": "Number",
      "id": true
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": ""
    },
    "suppliers": {
      "type": "hasMany",
      "model": "Supplier",
      "foreignKey": "",
      "through": "sel"
    },
    "addresses": {
      "type": "hasMany",
      "model": "Address",
      "foreignKey": ""
    },
    "cart": {
      "type": "hasOne",
      "model": "Cart",
      "foreignKey": "accountId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "edit"
    }
  ],
  "methods": {}
}
